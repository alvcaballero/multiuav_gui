table LaserScan {
  __metadata:fb.MsgMetadata;
  header:fb.std_msgs.Header (required);
  angle_min:float32;
  angle_max:float32;
  angle_increment:float32;
  time_increment:float32;
  scan_time:float32;
  range_min:float32;
  range_max:float32;
  ranges:[float32] (required);
  intensities:[float32] (required);
}

table CompressedImage {
  __metadata:fb.MsgMetadata;
  header:fb.std_msgs.Header (required);
  format:string (required);
  data:[uint8] (required);
}

table PointField {
  __metadata:fb.MsgMetadata;
  name:string (required);
  offset:uint32;
  datatype:uint8;
  count:uint32;
}

table PointCloud2 {
  __metadata:fb.MsgMetadata;
  header:fb.std_msgs.Header (required);
  height:uint32;
  width:uint32;
  fields:[PointField] (required);
  is_bigendian:bool;
  point_step:uint32;
  row_step:uint32;
  data:[uint8] (required);
  is_dense:bool;
}
 
