include "schema_common.fbs";

namespace fb.std_msgs;

table Header {
  __metadata:fb.MsgMetadata;
  seq:uint32;
  stamp:fb.RosTime (required);
  frame_id:string (required);
}

table String {
  __metadata:fb.MsgMetadata;
  data:string (required);
}

table Float64 {
  __metadata:fb.MsgMetadata;
  data:float64;
}
table Float32 {
  __metadata:fb.MsgMetadata;
  data: float32;
}
table UInt8 {
  __metadata:fb.MsgMetadata;
  data: uint8;
}